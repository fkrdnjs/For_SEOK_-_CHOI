//1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i=1;
    
    scanf("%d",&n);
    
    while(1){
        if(n%i == 0 && i != n)
            printf("%d ",i);
        if(i == n)
            printf("%d",i);
        i += 1;
        if(i > n)
            break;
        
    }
	return 0;
}

//2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int total = 0;
    
    while(1){
        scanf("%d",&n);
        
        if( n == 0)
            break;
            
        total += n;
    }
    
    printf("%d",total);
    
	return 0;
}

//3
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int k;
    int cnt = 0;
    
    scanf("%d",&n);
    
    do{
        scanf("%d",&k);
        
        if(k > n){
            printf("%d>?\n",k);
            cnt += 1;
        }
        else if(k < n){
            printf("%d<?\n",k);
            cnt+= 1;
        }
        else if(n == k){
            printf("%d==?\n",k);
            cnt += 1;
            break;
        }
        
    }while(1);
    
    printf("%d",cnt);
    
	return 0;
}

//4 - for 이용
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n, m;
    int tmp;
    int k;
    int i;
    
    scanf("%d %d",&n, &m);
    
    if(n > m){
        tmp = n;
        n = m;
        m = tmp;
    }
    
    for(i=n;i>=1;i--){
        if(n % i == 0 && m % i == 0){
            k = i;
            break;
        }
    }
    
    printf("%d",k);
    
	return 0;
}

//4 - while 이용
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n, m;
    int tmp;
    int k;
    int i;
    
    scanf("%d %d",&n, &m);
    
    if(n > m){
        tmp = n;
        n = m;
        m = tmp;
    }
    
    i = n;
    
    while(1){
        if(n % i == 0 && m % i == 0){
            k = i;
            break;
        }
        i -= 1;
    }
    
    printf("%d",k);
    
	return 0;
}

//5-1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i, j;
    
    scanf("%d",&n);
    
    for(i=0;i<n;i++){
        for(j=0;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    
	return 0;
}

//5-2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i, j;
    
    scanf("%d",&n);
    
    for(i=n-1;i>=0;i--){
        for(j=0;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    
	return 0;
}

//5-3
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i, j;
    
    scanf("%d",&n);
    
    for(i=1;i<=n;i++){
        for(j=1;j<=i*2-1;j++){
            printf("*");
        }
        printf("\n");
    }
    
	return 0;
}

//5-4
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i, j;
    
    scanf("%d",&n);
    
    for(i=n;i>=1;i--){
        for(j=1;j<=i*2-1;j++){
            printf("*");
        }
        printf("\n");
    }
    
	return 0;
}

//5
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    int i, j, k;
    
    scanf("%d",&n);
    
    for(i=1;i<=n;i++){
        for(k = n;k>i;k--)
            printf("  ");
        for(j=1;j<=i*2-1;j++){
            printf("*");
        }
        for(k = n;k>=i;k--)
            printf(" ");
        printf("\n");
    }
    
	return 0;
}

//6-1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
	int n;
	int i, j;
	
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
	    for(j=0;j<n;j++){
	        if(i == j || i + j == n-1)
	            printf(" X");
	        else
	            printf(" O");
	    }
	    printf("\n");
	}

	return 0;
}

//6-2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
	int n;
	int k = 1;
	int i, j;
	
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
	    for(j=0;j<n;j++){
	        if(k >=10)
	            k = k % 10;
	        printf(" %d",k);
	        k += 1;
	    }
	    printf("\n");
	}
	
	return 0;
}
