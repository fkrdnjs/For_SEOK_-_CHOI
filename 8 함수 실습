//1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int add(int x, int y);

int main()
{
	int n;
	int i;
	int result = 0;
	
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	    result = add(result, i);
	
    printf("%d",result);
    
	return 0;
}

int add(int x, int y){
    return x + y;
}

//2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int sum(int k);

int main()
{
	int a, b;
	int add;
	int sub;
	int result;
	
	scanf("%d %d",&a, &b);
	
	add = sum(b);
    sub = sum(a);
    result = add - sub + a;
    
    printf("%d",result);
    
	return 0;
}

int sum(int k){
    
    return k*(k+1)/2;
}

//3
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int sum(int k);

int main()
{
	int n;
	int i;
	int total = 0;
	
	scanf("%d",&n);
	
	for(i=1;i<=n;i++)
	    total += sum(i);
    
    printf("%d",total);
    
	return 0;
}

int sum(int k){
    
    return k*(k+1)/2;
}

//4 내일 수정해라
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define SIZE 5

void print_row1(int x);
void print_row2(int x);

int main()
{
	int n;
	int i;
	
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
	    if(i==0 || i == n -1)
	        print_row1(n);
	    else
	        print_row2(n);
	}

	return 0;
}

void print_row1(int x){
    int i;
    
    for(i=0;i<x;i++)
        printf("%d",x);
    printf("\n");
}

void print_row2(int x){
    int i;
    
    for(i=0;i<x;i++){
        if(i == 0 || i == x)
            printf("%d",x);
        else
            printf(" ");
    }
    printf("\n");
}
