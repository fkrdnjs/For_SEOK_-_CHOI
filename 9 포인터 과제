//1 출력 예시 오류라고 했던거 같긴한데 기억이 잘 안난다 내일 
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int maxmin(int ar[]){
    
    int *p = ar;
    int max = *p - 1;
    int min = *p + 1;
    
    for(p=ar; ;p++){
        if(*p == 0)
            break;
        if(*p >= max)
            max = *p;
        if(*p <= min)
            min = *p;
    }
    
    return max - min;
}

int main()
{
	int n;
	int ar[100];
	int *p = ar;
	int i;
	int result;
	
	scanf("%d",&n);
	
	for(i=0;i<n;i++){
	    for(p = ar; ;p++){
	        scanf("%d",p);
	        if(*p == 0)
	            break;
	    }
	    result = maxmin(ar);
	    
	    printf("result : %d\n",result);
	}
        
	return 0;
}



//2 다시풀기
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int x[6];
    int *p, *q, *r;
    int cnt = 0;
    int f = 0, check = 0;
    
    for(p=x;p<x+6;p++)
        scanf("%d",p);
        
    for(p=x;p<x+6;p++){
        if(p!=x){
            for(r=x;r<p;r++){
                if(*r == *p){
                    check = 1;
                    break;
                }
            }
        }
        if(check == 0){
            for(q=p;q<x+6;q++){
                if(*q == *p){
                    f = 1;
                    cnt += 1;
                }
            }
        }
        
        if(cnt >= 2)
            printf("%d %d\n",*p,cnt);
        
        check = 0;
        cnt = 0;
    }
    
    if(f == 0)
        printf("%d",f);
	   
	return 0;
}
