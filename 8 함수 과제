//1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void factor (int n){
    
    int i;
    
    for(i=1;i<=n;i++){
        if(n%i == 0)
            printf(" %d",i);
    }
    printf("\n");
}

int main(){
    
    int k;
    
    while(1){
        scanf("%d",&k);
        if( k <= 0)
            break;
        factor(k);
    }
    
    return 0;
}

//2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void triangle(int x){
    
    int i, j;
    
    for(i=1;i<=x;i++){
        for(j=x;j>i;j--)
            printf("  ");
        for(j=1;j<=2*i-1;j++)
            printf("*");
        printf("\n");
    }
}

void reverse(int x){
    
    int i, j;
    
    for(i=x;i>=1;i--){
        for(j=x;j>i;j--)
            printf("  ");
        for(j=1;j<=2*i-1;j++)
            printf("*");
        printf("\n");
    }
}

int main(){
    
    int n;
    
    scanf("%d",&n);
    
    if(n%2 == 0)
        triangle(n);
    else
        reverse(n);
    
    return 0;
}
