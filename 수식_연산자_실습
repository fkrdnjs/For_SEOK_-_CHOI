//1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    
    char c;
    
    scanf("%c",&c);
    
    if(c>='a' && c<='z')
        c = c + 'A' - 'a';
    else if(c>='A' && c<='Z')
        c = c + 'a' - 'A';
    
    printf("%c",c);
 
    return 0;
}

//2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
    
    int y;
    
    scanf("%d",&y);
    
    ((y % 4 == 0 && y % 100 !=0) || (y % 400 == 0)) ? printf("L") : printf("C");
 
    return 0;
}

//3
