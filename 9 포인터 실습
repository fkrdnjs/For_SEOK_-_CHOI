//1
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
	int x, y, z;
	int *px, *py, *pz, *tmp;
	
	px = &x;
	py = &y;
	pz = &z;
	
	scanf("%d %d %d",px,py,pz);
	
	tmp = pz;
	pz = py;
	py = px;
	px = tmp;
	
	printf("%d %d %d",*px, *py, *pz);

	return 0;
}

//2
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char ch[20];
    char *p = ch;
    char *q;
    
    scanf("%s",p);
    
    for(p=ch;*p!='\0';p++){
        if(*p == '#'){
            q = p - 1;
            break;
        }
    }
    
    for(p=q;p>=ch;p--)
        printf("%c",*p);
    
	return 0;
}

//3
